{x2;include:header}<body>{x2;include:nav}<div class="container-fluid pages-content">	<div class="row-fluid panels">		<div class="pep panels">			<div class="col-xs-2">				{x2;include:menu}			</div>			<div class="col-xs-10 nopadding">				<div class="panel panel-default pagebox border">					<div class="panel-heading bold">						站内信					</div>					<div class="panel-body">						<div class="panel-group">							<ul class="list-group">								{x2;tree:$msgs['data'],msg,mid}								<li class="list-group-item">									<span>{x2;v:msg['msgtitle']}</span>									<span class="pull-right" style="margin-top: 15px;">										<a class="btn btn-primary" href="index.php?user-app-msg-detail&msrid={x2;v:msg['msrid']}">查看</a>										<a class="btn btn-default confirm" href="index.php?user-app-msg-del&msrid={x2;v:msg['msrid']}">删除</a>									</span>									<br />									<span class="intros text-warning">来自：{x2;v:msg['msgposter']}</span>									<span class="intros text-warning"> {x2;v:msg['msgtime']}</span>									{x2;if:v:msg['msrstatus']}									<span class="intros"> 已读</span>									{x2;else}									<span class="intros text-warning"> 未读</span>									{x2;endif}								</li>								{x2;endtree}							</ul>						</div>                        {x2;if:$msgs['pages']}						<ul class="pagination pull-right">                            {x2;$msgs['pages']}						</ul>                        {x2;endif}					</div>				</div>			</div>		</div>	</div></div>{x2;include:footer}</body></html>