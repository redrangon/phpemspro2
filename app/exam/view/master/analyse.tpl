ist-unstyled list-inline">										<li><a href="index.php?exam-master-analyse-scores&username={x2;v:user['username']}&page={x2;$page}{x2;$u}" title="成绩记录">成绩记录</a></li>										<li><a href="index.php?exam-master-analyse-image&username={x2;v:user['username']}&page={x2;$page}{x2;$u}" title="能力画像">能力画像</a></li>									</ul>								</td>							</tr>							{x2;endtree}							</tbody>						</table>                        {x2;if:$users['pages']}						<ul class="pagination pull-right">                            {x2;$users['pages']}						</ul>                        {x2;endif}					</div>				</div>				{x2;include:footer}                {x2;if:!$_userhash}				            </div>		</div>	</div></div></body></html>{x2;endif}