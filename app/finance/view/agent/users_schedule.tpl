{x2;if:!$_userhash}{x2;include:header}<body>{x2;include:nav}<div class="container-fluid">	<div class="row-fluid">		<div class="pep">			<div class="col-xs-2 leftmenu">                {x2;include:menu}			</div>			<div id="datacontent">				{x2;endif}				<ol class="breadcrumb">					<li><a href="index.php?{x2;$_route['app']}-agent">代理平台</a></li>					<li><a href="index.php?{x2;$_route['app']}-agent-users">学员管理</a></li>					<li><a href="index.php?{x2;$_route['app']}-agent-users-status&username={x2;$username}">学习进度</a></li>					<li class="active">练习进度</li>				</ol>				<div class="panel panel-default" id="datacontent-container">					<div class="panel-heading">						{x2;$subject['subjectname']} - {x2;$username}					</div>					<div class="panel-body">						<table class="table table-hover table-bordered">							<tbody>								<tr class="info">									<th>名称</th>									<th width="120">做对</th>									<th width="120">已做</th>									<th width="120">总题数</th>									<th width="180">正确率</th>									<th width="210">进度条</th>								</tr>								{x2;tree:$basic['basicsections'],section,sid}								<tr>									<th colspan="6">{x2;$sections[v:section]['sectionname']}</th>								</tr>								{x2;tree:$basic['basicpoints'][v:section],point,pid}								<tr>									<td>										{x2;$points[v:section][v:point]['pointname']}									</td>									<td>										{x2;$right[v:point]}									</td>									<td>										{x2;$donenumber[v:point]}									</td>									<td>										{x2;$numbers[v:point]}									</td>									<td>										{x2;eval: echo intval(100 * $right[v:point] / $donenumber[v:point])} %									</td>									<td>										{x2;eval: v:rate = intval(100 * $donenumber[v:point]/$numbers[v:point])}										<div class="progress" style="margin-bottom: 0px;">											<div class="progress-bar" role="progressbar" aria-valuenow="{x2;v:rate}" aria-valuemin="0" aria-valuemax="100" style="width: {x2;v:rate}%;"></div>										</div>									</td>								</tr>								{x2;endtree}								{x2;endtree}							</tbody>						</table>					</div>				</div>				{x2;if:!$_userhash}				{x2;include:footer}			</div>		</div>	</div></div></body></html>{x2;endif}