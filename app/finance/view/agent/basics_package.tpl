{x2;include:header}<body>{x2;include:nav}<div class="container-fluid">	<div class="row-fluid">		<div class="pep">			<div class="col-xs-2 leftmenu">				{x2;include:menu}			</div>			<div id="datacontent">				<ol class="breadcrumb">					<li><a href="index.php?{x2;$_route['app']}-agent">代理平台</a></li>					<li class="active">开通课程</li>				</ol>				<div class="settingbar">					<a href="index.php?finance-agent-basics" class="btn btn-default">考场</a>					<a href="index.php?finance-agent-basics-package" class="btn btn-primary">套餐</a>				</div>				<div class="panel panel-default" id="datacontent-container">					<div class="panel-heading">套餐</div>					<div class="panel-body">						<form action="index.php?exam-master-exams" method="post" class="form-inline">							<table class="table">								<thead>									<tr>										<td width="120">											关键字：										</td>										<td>											<input class="form-control" name="search[keyword]" type="text" value="{x2;$search['keyword']}"/>										</td>										<td>											<button class="btn btn-primary" type="submit">搜索</button>											<input type="hidden" value="1" name="search[argsmodel]" />										</td>									</tr>								</thead>							</table>						</form>						<form action="index.php?finance-agent-basics-openpackage" method="post">							<table class="table table-bordered table-hover table-responsive package-{x2;v:trid} packageinfo">								<tr>									<th width="40"></th>									<th>类型名称</th>									<th>套餐名称</th>									<th width="80">时长</th>									<th width="120">价格</th>									<th width="120">原价</th>								</tr>								{x2;tree:$trainings['data'],training,trid}								{x2;tree:v:training['prices'],price,pid}								<tr>									<td>										<input class="packageradio" id="packageradio_{x2;v:training['trid']}" type="checkbox" name="delids[{x2;v:training['trid']}][{x2;v:key}]" value="1" data-price="{x2;v:price['agentprice']}">									</td>									<td><label for="packageradio_{x2;v:training['trid']}">{x2;v:training['trname']}</label></td>									<td><label for="packageradio_{x2;v:training['trid']}">{x2;v:price['name']}</label></td>									<td>{x2;v:price['time']}天</td>									<td>{x2;v:price['agentprice']}</td>									<td>{x2;v:price['price']}</td>								</tr>								{x2;endtree}								{x2;endtree}							</table>							<div class="form-group">								<h4 class="text-right col-xs-12">合计：<span id="totalprice">0</span> 元</h4>							</div>							<div class="form-group">								<div class="controls">									<span class="form-inline">手机号：<input type="text" needle="needle" class="form-control" datatype="cellphone" name="phonenumber" msg="请填写正确的手机号" placeholder="手机号，必填"/></span>									<span class="form-inline">&nbsp;&nbsp;姓名：<input type="text" class="form-control" name="name" placeholder="非必填"/></span>									<span class="form-inline">&nbsp;&nbsp;邮箱：<input type="text" class="form-control" name="email" placeholder="非必填"/></span>									<button class="btn btn-primary pull-right" type="submit">购买</button>								</div>							</div>						</form>						{x2;if:$trainings['pages']}						<ul class="pagination pull-right">							{x2;$trainings['pages']}						</ul>						{x2;endif}					</div>				</div>				{x2;include:footer}			</div>		</div>	</div></div></body></html>